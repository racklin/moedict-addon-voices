<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>moedict-addon-voice-t@moedict.tw</em:id>
        <em:name>MoeDict App Voice Pack (Taiwanese)</em:name>
        <em:version>1.0.0</em:version>
        <em:description>MoeDict App Voice Pack (Taiwanese)</em:description>
        <em:creator>Rack Lin(racklin@gmail.com)</em:creator>
        <em:homepageURL>https://github.com/racklin/</em:homepageURL>
        <em:type>2</em:type>
        <em:unpack>true</em:unpack>

    <!-- Gecko Toolkit
         version 1.9.0 and higher -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9a1</em:minVersion>
        <em:maxVersion>32.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:localized>
      <Description>
        <em:locale>zh</em:locale>
        <em:name>萌典離線版語音 － 台灣閩南語</em:name>
        <em:description>萌典離線版語音 － 台灣閩南語</em:description>
      </Description>
    </em:localized>

    </Description>
</RDF>

